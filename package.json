{
  "name": "pattern-flow",
  "version": "1.0.0",
  "description": "A toolkit for non-linear sequencing with @most/core",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-deno"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "scripts": {
    "build": "@pika/pack",
    "test": "ts-node node_modules/tape/bin/tape",
    "unit-test": "mocha -r ts-node/register 'src/**/*.test.ts'",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "precommit": "lint-staged",
    "preversion": "npm test && npm run build",
    "postversion": "npm publish --access=public",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micahscopes/pattern-flux.git"
  },
  "keywords": [
    "most",
    "@most/core",
    "mostjs",
    "scheduler",
    "midi",
    "tempo",
    "sequencing"
  ],
  "author": "micahscopes@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micahscopes/pattern-flow/issues"
  },
  "homepage": "https://github.com/micahscopes/pattern-flow#readme",
  "dependencies": {
    "@most/core": "^1.6.1",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-deno": "^0.9.2",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@typed/fp": "^0.14.0",
    "fp-ts": "^2.11.2",
    "most-pulsing-scheduler": "git+https://github.com/micahscopes/pulsing-scheduler.git#main",
    "sinon": "^11.1.2",
    "tape": "^5.3.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-pipeline-operator": "^7.15.0",
    "@types/tape": "^4.13.2"
  }
}
